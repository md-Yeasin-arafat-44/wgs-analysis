pip install nanoplot


NanoPlot -t 2 \
         -o /mnt/d/Paper_Reproduction/02_Quality_Control/nanopolish \
         -p nstu_plot \
         --loglength \
         -f png \
         --plots dot,hex,kde \
         --title "nstu_reads" \
         --fastq /mnt/d/Paper_Reproduction/01_Raw_Data/nstu_reads_fastq.gz
